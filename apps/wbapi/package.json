{
    "name": "@warbrokers/wbapi",
    "version": "0.0.0",
    "main": "./build/index.mjs",
    "private": true,
    "scripts": {
        "health-check": "node health-check.mjs",
        "dev": "pnpm build && concurrently -c \"gray,white\" \"cd docker && docker compose up\" \"pnpm start:dev\"",
        "build": "rm -rf build && tsup && tsc --emitDeclarationOnly",
        "start:dev": "NODE_ENV=development node .",
        "start:prod": "node .",
        "lint": "eslint . && tsc --noEmit",
        "test:unit": "jest",
        "clean": "rm -rf .turbo build node_modules *.log"
    },
    "dependencies": {
        "@warbrokers/types": "workspace:*",
        "@trpc/server": "^10.45.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.29.4",
        "drizzle-zod": "^0.5.1",
        "express": "^4.20.0",
        "postgres": "^3.4.3",
        "swagger-ui-express": "^5.0.0",
        "trpc-openapi": "^1.2.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@warbrokers/eslint-config": "workspace:*",
        "@swc/core": "^1.4.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/swagger-ui-express": "^4.1.6",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "openapi-types": "^12.1.3",
        "ts-jest": "^29.1.2",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "pnpm": ">=9.0",
        "node": ">=20.0"
    }
}
